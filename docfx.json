{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.yml"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "SECURITY.md",
          "CODE_OF_CONDUCT.md",
          "ThirdPartyNotices",
          "tutorials/includes/dotnet/src/**",
          "tutorials/includes/go/src/**",
          "tutorials/includes/java/src/**",
          "tutorials/includes/javascript/src/**",
          "tutorials/includes/php/src/**",
          "tutorials/includes/power-automate/src/**",
          "tutorials/includes/powershell/src/**",
          "tutorials/includes/python/src/**",
          "tutorials/includes/typescript/src/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "_themes/**",
          "_themes.pdf/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/graph/breadcrumb/toc.json",
      "extendBreadcrumb": true,
      "feedback_system": "Standard",
      "uhfHeaderId": "MSDocsHeader-MSGraph",
      "ms.subservice": "non-product-specific",
      "ms.service": "microsoft-graph"
    },
    "fileMetadata": {
      "titleSuffix": {
        "**/*.md": "Microsoft Graph"
      }
    },
    "template": [],
    "dest": "graph-training",
    "markdownEngineName": "markdig"
  }
}
